{
  "accessibility.signals.terminalBell": {
    "sound": "off"
  },
  "editor.tabSize": 2,
  "editor.formatOnSave": true,
  "rust-analyzer.cargo.buildScripts.enable": true,
  // "rust-analyzer.linkedProjects": [
  //   "crates/Cargo.toml"
  // ],
  "accessibility.signalOptions.volume": 0,
  "[sql]": {
    "editor.defaultFormatter": "ReneSaarsoo.sql-formatter-vsc"
  },
  "SQL-Formatter-VSCode.ignoreTabSettings": true,
  "SQL-Formatter-VSCode.dataTypeCase": "lower",
  "SQL-Formatter-VSCode.dialect": "postgresql",
  "SQL-Formatter-VSCode.expressionWidth": 110,
  "SQL-Formatter-VSCode.functionCase": "lower",
  "SQL-Formatter-VSCode.identifierCase": "lower",
  "SQL-Formatter-VSCode.keywordCase": "lower",
  "python.terminal.activateEnvironment": false,
  "cSpell.words": [
    "aliri",
    "appender",
    "asserhttp",
    "backon",
    "chrono",
    "croner",
    "dashmap",
    "dotenvy",
    "fusiondata",
    "guixu",
    "guixus",
    "hmac",
    "ilike",
    "itertools",
    "jemallocator",
    "josekit",
    "minijinja",
    "modelsql",
    "opendal",
    "pgvector",
    "prost",
    "qrcode",
    "repr",
    "reqwest",
    "rmcp",
    "sched",
    "serde",
    "sqlx",
    "thiserror",
    "tikv",
    "timestamptz",
    "uidx",
    "unplugin",
    "urlencoding",
    "varchar"
  ],
  "SQL-Formatter-VSCode.indentStyle": "standard"
}
