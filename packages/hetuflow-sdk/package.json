{
  "name": "@fusion-data/hetuflow-sdk",
  "version": "1.0.0",
  "description": "Hetuflow TypeScript SDK for API access",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "generate": "openapi-typescript http://localhost:9500/docs/openapi.json -o src/types/openapi.ts",
    "format": "prettier --write .",
    "format:check": "prettier --check ."
  },
  "keywords": [
    "hetuflow",
    "sdk",
    "typescript",
    "api"
  ],
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.12.2",
    "@fusion-data/modelsql": "workspace:*",
    "@fusion-data/fusion-core": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "jest": "^30.1.3",
    "typescript": "^5.9.2",
    "eslint": "^9.36.0",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.3.3"
  },
  "files": [
    "dist"
  ]
}
