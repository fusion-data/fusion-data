{
  "name": "@fusion-data/hetumind-sdk",
  "version": "1.0.0",
  "description": "Hetumind TypeScript SDK for AI Agent/Flow platform API access",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "clean": "rm -rf dist",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "generate": "openapi-typescript http://localhost:3000/docs/openapi.json -o src/types/openapi.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "hetumind",
    "sdk",
    "typescript",
    "api",
    "ai",
    "agent",
    "workflow",
    "llm"
  ],
  "author": "Fusion Data Team",
  "license": "Apache-2.0",
  "homepage": "https://github.com/fusion-data/fusion-data",
  "repository": {
    "type": "git",
    "url": "https://github.com/fusion-data/fusion-data.git",
    "directory": "packages/hetumind-sdk"
  },
  "bugs": {
    "url": "https://github.com/fusion-data/fusion-data/issues"
  },
  "dependencies": {
    "axios": "^1.12.2",
    "dayjs": "^1.11.10",
    "@fusion-data/fusion-core": "workspace:*",
    "@fusion-data/fusionsql": "workspace:*"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^22.0.0",
    "jest": "^30.1.3",
    "typescript": "^5.9.2",
    "eslint": "^9.36.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "openapi-typescript": "^7.9.1",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.0"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
